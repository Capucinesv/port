<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.port.HelloController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="122.0" prefWidth="754.0" text="Porte Feuille" textAlignment="CENTER" textFill="RED" textOverrun="WORD_ELLIPSIS">
    <font>
      <Font name="Niagara Solid" size="96.0" />
    </font>
  </Label>

  <Label fx:id="welcomeText" />
  <Button id="button" onAction="#onHelloButtonClick" prefHeight="121.0" prefWidth="378.0" text="Start" textAlignment="CENTER">
    <font>
      <Font size="37.0" />
    </font>
    <effect>
      <ColorAdjust contrast="-0.05" hue="0.02" saturation="1.0">
        <input>
          <DropShadow />
        </input></ColorAdjust>
    </effect>
    <cursor>
      <Cursor fx:constant="HAND" />
    </cursor></Button>
</VBox>